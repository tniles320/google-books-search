{
  "name": "google-books-search",
  "version": "1.0.0",
  "description": "React-based Google Books Search app",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && set HTTPS=true && npm run start:prod || set HTTPS=true && npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "node seeders/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tniles320/google-books-search.git"
  },
  "author": "Taylor Niles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tniles320/google-books-search/issues"
  },
  "homepage": "https://github.com/tniles320/google-books-search#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.11.12",
    "nodemon": "^2.0.7"
  }
}
